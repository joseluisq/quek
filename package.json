{
  "name": "quek",
  "version": "1.1.0",
  "description": "A pretty small ES6 API for Queue and Stack ADTs.",
  "repository": "joseluisq/quek",
  "license": "MIT",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "keywords": [
    "queue",
    "stack",
    "pop",
    "shift",
    "push",
    "ADT",
    "collection",
    "enqueue",
    "dequeue",
    "peek",
    "es6"
  ],
  "engines": {
    "node": ">= 4"
  },
  "main": "./quek.js",
  "scripts": {
    "start": "webpack-dev-server -d --content-base ./ --inline quek.js",
    "build:lib": "babel ./src -d . --ignore 'test.js'",
    "build:umd": "webpack src/quek.js dist/quek.js",
    "build:min": "webpack -p src/quek.js dist/quek.min.js",
    "build": "node ./scripts/build.js",
    "prepublish": "node ./scripts/build.js",
    "release": "node ./scripts/release.js",
    "lint": "eslint src",
    "test": "npm run lint && jest"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "eslint": "^3.11.1",
    "gzip-size": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest-cli": "^17.0.3",
    "pretty-bytes": "^4.0.2",
    "readline-sync": "^1.4.5",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": ["es2015", "stage-1"],
    "plugins": ["dev-expression"]
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": ["/(node_modules|scripts|dist)/"],
    "testPathDirs": ["src"],
    "testRegex": "test.(js|jsx|spec)$"
  }
}

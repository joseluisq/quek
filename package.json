{
  "name": "quesk",
  "version": "2.0.0",
  "description": "A pretty small ES6 API for Queue and Stack ADTs.",
  "repository": "joseluisq/quesk",
  "license": "MIT",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "keywords": [
    "queue",
    "stack",
    "pop",
    "shift",
    "push",
    "ADT",
    "collection",
    "enqueue",
    "dequeue",
    "peek",
    "es6"
  ],
  "engines": {
    "node": ">= 4"
  },
  "main": "dist/quesk.js",
  "scripts": {
    "version": "npm run build",
    "build": "rm -rf dist && npm run js:minified && npm run js:unminified",
    "js:minified": "cross-env NODE_ENV=minified rollup -c",
    "js:unminified": "cross-env NODE_ENV=unminified rollup -c",
    "lint": "eslint index.js test.js",
    "test": "npm run lint && jest test.js --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.44.2",
    "jest-cli": "^19.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "env": {
      "jest": true
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/(node_modules|scripts|dist)/"
    ],
    "testRegex": "test.(js|jsx|spec)$"
  }
}
